{
    "name": "chronex/cookiebanner",
    "description": "A flexible, customizable GDPR-compliant cookie consent banner with automatic script blocking, multiple templates, and comprehensive event hooks.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Murat Alabacak",
            "homepage": "https://github.com/alabacakmurat"
        }
    ],
    "keywords": [
        "gdpr",
        "cookie",
        "consent",
        "banner",
        "privacy",
        "cookie-consent",
        "cookie-banner",
        "gdpr-compliance"
    ],
    "homepage": "https://github.com/alabacakmurat/cookiebanner",
    "support": {
        "issues": "https://github.com/alabacakmurat/cookiebanner/issues",
        "source": "https://github.com/alabacakmurat/cookiebanner"
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Chronex\\CookieBanner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chronex\\CookieBanner\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src --level=8",
        "publish-assets": "Chronex\\CookieBanner\\Asset\\ComposerScripts::publishAssets"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "cookiebanner": {
            "publish-path": "public/vendor/cookiebanner",
            "minify": false,
            "combined": false
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
